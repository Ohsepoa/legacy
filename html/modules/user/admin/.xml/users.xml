<?xml version="1.0" encoding="utf-8" ?>
<xoopscube-dynaactionform>
	<form-bean name="UserAdminEditForm" type="XCube_ActionForm">
		<form-property name="uid" type="Int" />
		<form-property name="name" type="String" />
		<form-property name="uname" type="String" />
		<form-property name="email" type="String" />
		<form-property name="url" type="String" />
		<form-property name="user_icq" type="String" />
		<form-property name="user_from" type="String" />
		<form-property name="user_sig" type="Text" />
		<form-property name="user_viewemail" type="Int" />
		<form-property name="user_aim" type="String" />
		<form-property name="user_yim" type="String" />
		<form-property name="user_msnm" type="String" />

		<form-property name="pass" type="String" />
		<form-property name="vpass" type="String" />

		<form-property name="posts" type="Int" />
		<form-property name="attachsig" type="Int" />
		<form-property name="rank" type="Int" />
		<form-property name="level" type="Int" />
		<form-property name="timezone_offset" type="Float" />

		<form-property name="umode" type="String" />
		<form-property name="uorder" type="Int" />

		<form-property name="notify_method" type="Int" />
		<form-property name="notify_mode" type="Int" />

		<form-property name="user_occ" type="String" />
		<form-property name="bio" type="Text" />
		<form-property name="user_intrest" type="String" />
		<form-property name="user_mailok" type="Int" />

		<form-property name="groups" type="Int[]" />
	</form-bean>
	<form-validation>
		<field property="uid" depends="required">
			<arg0 key="const._MD_USER_LANG_UID" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>
		<field property="name" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_NAME" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="60" />
		</field>
		<field property="uname" depends="required,maxlength">
			<arg0 key="const._MD_USER_LANG_UNAME" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="25" />
		</field>
		<field property="email" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_EMAIL" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="60" />
		</field>
		<field property="url" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_URL" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="100" />
		</field>
		<field property="user_icq" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_ICQ" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="15" />
		</field>
		<field property="user_from" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_FROM" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="100" />
		</field>
		<field property="user_sig" depends="">
			<arg0 key="const._MD_USER_LANG_USER_SIG" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>
		<field property="user_viewemail" depends="">
			<arg0 key="const._MD_USER_LANG_USER_VIEWEMAIL" />
		</field>
		<field property="user_aim" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_AIM" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="18" />
		</field>
		<field property="user_yim" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_YIM" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="25" />
		</field>
		<field property="user_msnm" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_MSNM" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="100" />
		</field>

		<field property="pass" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_PASS" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="32" />
		</field>
		<field property="vpass" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_PASS" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="32" />
		</field>

		<field property="posts" depends="required">
			<arg0 key="const._MD_USER_LANG_POSTS" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>
		<field property="attachsig" depends="">
		</field>
		<field property="rank" depends="required">
			<arg0 key="const._MD_USER_LANG_RANK" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>
		<field property="level" depends="required">
			<arg0 key="const._MD_USER_LANG_LEVEL" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>
		<field property="timezone_offset" depends="required">
			<arg0 key="const._MD_USER_LANG_TIMEZONE_OFFSET" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>

		<field property="umode" depends="required">
			<arg0 key="const._MD_USER_LANG_UMODE" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
		</field>
		<field property="uorder" depends="required,intRange">
			<arg0 key="const._MD_USER_LANG_UORDER" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
			<msg name="intRange" key="const._MD_USER_ERROR_INJURY" />
			<var name="min" value="0" />
			<var name="max" value="1" />
		</field>

		<field property="notify_method" depends="required,intRange">
			<arg0 key="const._MD_USER_LANG_NOTIFY_METHOD" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
			<msg name="intRange" key="const._MD_USER_ERROR_INJURY" />
			<var name="min" value="0" />
			<var name="max" value="2" />
		</field>
		<field property="notify_mode" depends="required,intRange">
			<arg0 key="const._MD_USER_LANG_NOTIFY_MODE" />
			<msg name="required" key="const._MD_USER_ERROR_REQUIRED" />
			<msg name="intRange" key="const._MD_USER_ERROR_INJURY" />
			<var name="min" value="0" />
			<var name="max" value="2" />
		</field>

		<field property="user_occ" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_OCC" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="100" />
		</field>
		<field property="bio" depends="">
			<arg0 key="const._MD_USER_LANG_BIO" />
		</field>
		<field property="user_intrest" depends="maxlength">
			<arg0 key="const._MD_USER_LANG_USER_INTREST" />
			<arg1 key="${var:maxlength}" />
			<msg name="maxlength" key="const._MD_USER_ERROR_MAXLENGTH" />
			<var name="maxlength" value="150" />
		</field>
		<field property="user_mailok" depends="">
			<arg0 key="const._MD_USER_LANG_USER_MAILOK" />
		</field>
	</form-validation>
</xoopscube-dynaactionform>
